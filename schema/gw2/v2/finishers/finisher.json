{
  "$id": "schema/gw2/v2/finishers/finisher.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "properties": {
    "icon": { "format": "uri", "minLength": 1, "type": "string" },
    "id": { "type": "integer" },
    "name": { "minLength": 1, "type": "string" },
    "order": { "type": "integer" },
    "unlock_details": { "type": "string" },
