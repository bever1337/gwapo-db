{
  "$id": "gw2/v2/jadebots/index.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "oneOf": [
    {
      "items": { "type": "integer" },
      "type": "array"
    },
    {
      "properties": {
        "description": { "type": "string" },
        "id": { "type": "integer" },
        "name": { "minLength": 1, "type": "string" },
        "unlock_item": { "type": "integer" }
