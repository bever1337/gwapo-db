{
  "$id": "gw2/v2/minis/index.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "oneOf": [
    {
      "items": { "type": "integer" },
      "type": "array"
    },
    {
      "properties": {
        "icon": { "format": "uri", "minLength": 1, "type": "string" },
        "id": { "type": "integer" },
        "item_id": { "type": "integer" },
        "name": { "minLength": 1, "type": "string" },
        "order": { "type": "integer" },
        "unlock": { "minLength": 1, "type": "string" }
      },
      "required": ["icon", "id", "item_id", "name", "order"],
      "type": "object"
    }
  ]
}
